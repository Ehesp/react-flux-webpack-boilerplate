{
  "name": "react-flux-webpack-boilerplate",
  "version": "1.0.0",
  "description": "React, Flux (Alt) & Webpack boilerplate",
  "scripts": {
    "dev": "node server.js",
    "prod": "npm run build && NODE_ENV=production node server.js",
